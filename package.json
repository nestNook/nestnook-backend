{
  "name": "nestnook-backend",
  "version": "1.0.0",
  "description": "Backend of NestNook website",
  "main": "main.js",
  "scripts": {
    "test": "jest --detectOpenHandles --passWithNoTests",
    "test:component": "cucumber-js",
    "test:mutation": "stryker run",
    "prepare": "husky install",
    "start": "node -r tsconfig-paths/register ./dist/main.js",
    "start:dev": "tsnd -r tsconfig-paths/register --rs --respawn --transpile-only ./src/main.ts",
    "build": "tsc -p tsconfig.build.json && tsc-alias",
    "gm": "sh ./scripts/generate.sh",
    "lint": "eslint ./src/**",
    "deps": "madge ./src -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pedrobraghin/nestnook-backend.git"
  },
  "author": "Pedro Braghin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pedrobraghin/nestnook-backend/issues"
  },
  "homepage": "https://github.com/pedrobraghin/nestnook-backend#readme",
  "devDependencies": {
    "@cucumber/cucumber": "^9.4.0",
    "@faker-js/faker": "^8.0.2",
    "@jest/globals": "^29.6.3",
    "@stryker-mutator/core": "^7.1.1",
    "@stryker-mutator/jest-runner": "^7.1.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.4",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.5.3",
    "@types/sinon": "^10.0.16",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.0",
    "jest": "^29.6.3",
    "madge": "^6.1.0",
    "prettier": "3.0.3",
    "prisma": "^5.2.0",
    "sinon": "^15.2.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@prisma/client": "^5.1.1",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.0",
    "zod": "^3.22.2"
  }
}
